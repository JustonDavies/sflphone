	@echo "Indenting code:"
	if [ -f $(ASTYLERC) ] ; then \
		find $(top_srcdir)/src/ -regex ".*\.\(h\|c\)" -exec $(indent) --options=$(ASTYLERC) {} \; ; \
	fi

